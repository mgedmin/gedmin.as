## Script (Python) "taken"
##bind container=container
##bind context=context
##bind namespace=
##bind script=script
##bind subpath=traverse_subpath
##parameters=taskid
##title=
##
tasks = []
for persondata in container.parseTasks().values():
    task = persondata.get(taskid, {}).get('description')
    done = persondata.get(taskid, {}).get('done')
    if task:
        tasks.append((task, persondata['name'], done))
tasks.sort()
return [{'task': task, 'student': name, 'done': done,
         'done_suffix': done and ' [+]' or ''}
        for task, name, done in tasks]
