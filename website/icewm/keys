key "Alt+Ctrl+t" Eterm
key "Alt+Ctrl+s" Eterm -n "Root Terminal" -e su -
key "Alt+Ctrl+a" Eterm -n "Admin Terminal" -e su - admin
key "Alt+Ctrl+k" xkill
key "Alt+Ctrl+m" Eterm -t mutt
key "Alt+Ctrl+g" Eterm -t OmniMail --pause -e om
